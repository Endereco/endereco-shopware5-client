    <elements>
        <element type="text" scope="shop">
            <name>apiKey</name>
            <label lang="de">Zugriffsschlüssel (kann man bei info@endereco.de anfordern)</label>
            <label lang="en">API-Key (you get it from info@endereco.de)</label>
            <description lang="de">Zugriffsschlüssel für die Endereco Schnittstelle.</description>
            <description lang="en">Access key for the Endereco API.</description>
        </element>

        <element type="boolean" scope="shop">
            <name>isPluginActive</name>
            <label lang="de">Plugin ist aktiv</label>
            <label lang="en">Plugin is active</label>
            <value>true</value>
        </element>

        <element type="button">
            <name>textKey</name>
            <label lang="de">Zugriffsschlüssel prüfen</label>
            <label lang="en">Check API key</label>
            <options>
                <handler lang="de">
                    <![CDATA[
                    function() {
                      var $apiKey = this.el.dom.closest('table').querySelector('tbody').querySelector('input').value;
                      Ext.Ajax.request({
                        url: 'EnderecoShopware5Client/testApi?apiKey='+$apiKey,
                        success: function (response) {
                            try {
                                var responseO = JSON.parse(response.responseText)
                                Shopware.Msg.createGrowlMessage(response.statusText, responseO.response)
                            } catch (e) {
                                // There is no json here, probably user clicked on the button, before the controller is visible.
                            }
                        },
                        failure: function (response) {
                            try {
                                var responseO = JSON.parse(response.responseText)
                                Shopware.Msg.createGrowlMessage(response.statusText, responseO.response)
                            } catch (e) {
                                // There is no json here, probably user clicked on the button, before the controller is visible.
                            }
                        }
                      });
                    }
                    ]]>
                </handler>
            </options>
        </element>

        <element type="text" scope="shop">
            <name>remoteApiUrl</name>
            <label lang="de">URL für remote API</label>
            <label lang="en">Remote API Url</label>
            <value>https://endereco-service.de/rpc/v1</value>
            <description lang="de">URL der Endereco API.</description>
            <description lang="en">Access URL for the Endereco API.</description>
        </element>

